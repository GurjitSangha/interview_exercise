{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "start:dev": {
      "dependsOn": ["^start:dev"],
      "cache": false,
      "persistent": true
    },
    "lint": {
      "outputs": []
    }
  }
}
